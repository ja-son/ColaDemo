    {
      "or": [
        {
          "equals": {
            "method": "GET",
            "path": "\/api\/questions",
            "query": {
              "tagged": "asp.net"
            }
          },
          "headers": {
            "Accept": "application\/json"
          }
        },
        {
          "equals": {
            "method": "GET",
            "path": "\/api\/questions",
            "query": {
              "inTitle": "asp.net"
            }
          },
          "headers": {
            "Accept": "application\/json"
          }
        }
      ]
    }
  ],
  "responses": [
    {
      "inject":  "<%- stringify(filename, 'questions_response.ejs') %>",
      "_behaviors": {
        "wait": 500
      }
    }
  ]
}